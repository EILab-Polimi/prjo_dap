use Drupal\Core\Url;

/**
 * Implements hook_help().
 */
function qgis_server_http_client_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the qgis_server_http_client module.
    case 'help.page.qgis_server_http_client':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('My Awesome Module') . '</p>';
      return $output;

    default:
  }
}

